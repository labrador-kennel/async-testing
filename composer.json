{
  "name": "cspray/labrador-async-unit",
  "description": "An async unit and integration testing framework written on top of PHP8",
  "bin": [
    "bin/asyncunit"
  ],
  "require": {
    "php": "^8.0",
    "amphp/amp": "^2",
    "nikic/php-parser": "^4.10",
    "cspray/labrador-async-event": "^2.3",
    "cspray/labrador": "^3.2.0-alpha3",
    "symfony/console": "^5.2",
    "opis/json-schema": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Cspray\\Labrador\\AsyncUnit\\": "framework_src",
      "Cspray\\Labrador\\AsyncUnit\\CliTool\\": "cli_tool_src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cspray\\Labrador\\AsyncUnit\\": "framework_test",
      "Cspray\\Labrador\\AsyncUnit\\CliTool\\": "cli_tool_test",
      "Acme\\DemoSuites\\": "acme_src",
      "Acme\\Examples\\SimpleEquals\\": "examples/simple_equals_src"
    }
  }
}
